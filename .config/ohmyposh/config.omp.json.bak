{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 4,
  "final_space": true,
  "async": true,
  "blocks": [
    {
      "type": "rprompt",
      "alignment": "left",
      "segments": [
        {
          "type": "path",
          "style": "plain",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:text-light",
          "background": "p:path-bg",
          "template": " \uf07c {{ .Path }} ",
          "options": {
            "style": "folder"
          }
        },
        {
          "type": "git",
          "style": "plain",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:text-light",
          "background": "p:git-bg",
          "template": " \uf126 {{ .HEAD }}{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }} "
        },
        {
          "type": "project",
          "style": "plain",
          "powerline_symbol": "\ue0b0",
          "template": " \ueb30 {{ .Name }}{{ if .Version }} {{ .Version }}{{ end }} ",
          "foreground": "p:text-light",
          "background": "p:git-bg"
        },
        {
          "type": "root",
          "style": "plain",
          "powerline_symbol": "\ue0b0",
          "template": " \uf0e7 ",
          "foreground": "p:text-light",
          "background": "p:git-bg"
        },
        {
          "type": "os",
          "style": "plain",
          "powerline_symbol": "\ue0b0",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "cache": {
            "duration": "24h",
            "strategy": "session"
          },
          "foreground": "p:text-light",
          "background": "p:git-bg"
        }
      ],
      "newline": true
    }
  ],
  "palette": {
    "text-light": "#ffffff"
  }
}